{
  "version": 2,
  "routes": [
    { "src": "/api/ws", "dest": "/api/ws.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
