{
  "version": 2,
  "rewrites": [
    { "source": "/api/ws", "destination": "/api/ws.js" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
